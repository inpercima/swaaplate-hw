{
  "backend": {
    "javaKt": {
      "management": "",
      "packagePath": ""
    },
    "language": "php",
    "php": {
      "modRewritePhpExtension": false,
      "runAsApi": true
    }
  },
  "frontend": {
    "buildDir": "dist",
    "ghUser": "inpercima",
    "installDependencies": true,
    "language": "en",
    "modules": {
      "enabled": true,
      "features": {
        "defaultRoute": "hello-world",
        "name": "features"
      },
      "notFound": {
        "enabled": true,
        "name": "not-found"
      },
      "routing": true
    },
    "packageJson": {
      "contributors": [{
        "name": "Marcel Jänicke",
        "email": "inpercima@gmail.com"
      }],
      "homepage": "https://github.com/inpercima/swaaplate-hw",
      "repository": "https://github.com/inpercima/swaaplate-hw"
    },
    "prefix": "hw",
    "theme": "indigo-pink",
    "useGoogleFonts": true,
    "useYarn": false
  },
  "general": {
    "author": "Marcel Jänicke",
    "description": "Example app with default options but useYarn: false, backend: php, modRewriteIndex: true, serverAsApi: true.",
    "modRewriteIndex": true,
    "name": "hello-world-php-npm-api-htaccess",
    "title": "Hello world with php, npm, api folder and htaccess file",
    "useDocker": false,
    "useMITLicense": true,
    "useMock": false,
    "useSecurity": false
  }
}

{
  "general": {
    "author": "Marcel Jänicke",
    "description": "An angular web project by swaaplate with java, yarn and maven.",
    "github": {
      "use": true,
      "username": "inpercima"
    },
    "name": "java-yarn-maven",
    "title": "java-yarn-maven",
    "useDocker": false
  },
  "client": {
    "buildDir": "dist",
    "installDependencies": true,
    "packageJson": {
      "contributors": [{
        "name": "Marcel Jänicke",
        "email": "inpercima@gmail.com"
      }],
      "homepage": "https://github.com/inpercima/swaaplate-hw/java-yarn-maven",
      "repository": "https://github.com/inpercima/swaaplate-hw/java-yarn-maven"
    },
    "route": {
      "features": {
        "default": "dashboard",
        "show": true
      },
      "login": {
        "activate": true,
        "name": "login",
        "show": false
      },
      "notFound": {
        "name": "not-found",
        "redirect": false
      }
    },
    "selectorPrefix": "jym",
    "theme": "deeppurple-amber",
    "useYarn": true
  },
  "server": {
    "backend": "java",
    "htaccess": false,
    "management": "maven",
    "packagePath": "net.inpercima.swaaplatehw.jym",
    "serverAsApi": true
  }
}
